    <section  class="h-100 mt-4  ">
      <h2 class="text-center">Ordered Products</h2>
    
      <div class="container  table-responsive">
        <table class="table shadow  ">
          <thead class="table-dark">
            <tr>
              <th scope="col">Product Image</th>
              <th scope="col">Product Name</th>
              <th scope="col"> Product description</th>
              <th scope="col">Product Price</th>
              <th scope="col"> Product quantity</th>

            </tr>
          </thead>
          <tbody class="searchable">
            <%for(let i=0;i < orderdItems.length;i++){%>
            <tr>
              <td><img src="/<%=orderdItems[i].product.image[0]%>" width="100px"  height="100px" class="img-fluid" alt="<%=orderdItems[i].product.product_name%>"></td>
              <td><%=orderdItems[i].product.product_name%>"</td>
              <td><%=orderdItems[i].product.desc%></td>
              <td><%=orderdItems[i].product.price%></td>
              <td><%=orderdItems[i].quantity%></td>
          
            </tr>
            <%}%>
        
          </tbody>
        </table>
      </div>
    </section>
    
