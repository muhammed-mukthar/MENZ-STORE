	<script type="text/javascript">
		 function changeQuantity(cartId, proId, userId,count) {
        const quantity = parseInt(document.getElementById(proId).innerHTML)
   
        // count = parseInt(count)
      
        $.ajax({
          url: '/users/change-product-quantity',
          data: {
            user: userId,
            cart: cartId,
            product: proId,
            count: count,
            quantity: quantity
          },
          method: 'post',
          success: (res) => {
		
				if(res.removeproduct){

					swal.fire({
						showCancelButton: false, // There won't be any cancel button
						showConfirmButton: false ,
						title:'product has been removed from the cart',
						
					})
					

					setTimeout(()=>{
						location.reload()
						
					},2000)
				
				}else{
					document.getElementById(proId).innerHTML=quantity+count;
          document.getElementById('totalvalue').innerHTML=res.totals;
		  document.getElementById('fulltotal').innerHTML=res.totals;
				}
          }
        })
      }

	  function  deleteproduct(cartId, proId, userId){
		const quantity = parseInt(document.getElementById(proId).innerHTML)

		


				Swal.fire({
            title: 'Are you want to Remove?',
            icon: 'warning',
            showCancelButton: true,
            confirmButtonColor: '#3085d6',
            cancelButtonColor: '#d33',
            confirmButtonText: 'Remove'
        }).then((result) => {
          if(result.isConfirmed){
            $.ajax({
			url:'/users/cart/remove',
			data:{
				user: userId,
            cart: cartId,
            product: proId,
         
            quantity: quantity
			},
			method: 'post',
			success:(res)=>{
    
          location.reload()
     
			
			}
            
        })



          }
           
		
    
				
				

			})
		}

		


		  </script>

