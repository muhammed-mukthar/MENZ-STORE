<%-include('../include/adminheader.ejs')-%>

  <body>
    
    <nav class="navbar navbar-expand-lg navbackground mb-5">
      <div class="container-fluid ">
        <a class="navbar-brand" href="/admin">Admin </a>
        <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
          <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse justify-content-center" id="navbarNav">
          <ul class="navbar-nav">
            <li class="nav-item">
              <a class="nav-link "  href="/admin">dashboard</a>
            </li>
            <li class="nav-item">
              <a class="nav-link" href="/admin/orders">Orders</a>
            </li>
            <li class="nav-item">
              <a class="nav-link "  href="/admin/products">Products</a>
            </li>
            <li class="nav-item">
              <a href="/admin/usermanagement" aria-current="page" class="nav-link active">Users</a>
            </li>
            <li class="nav-item">
              <a href="/admin/category" class="nav-link ">Category</a>
            </li>
            <li class="nav-item">
              <a href="/admin/logout" class="nav-link ">Sign out</a>
            </li>
          </ul>
        </div>
      </div>
    </nav>

    <div class="container mt-3">
      <table id="example" class="display responsive nowrap" style="width:100%">
        <thead class="theadclass">
       
          <tr>
           
            <th scope="col">Name</th>
            <th scope="col">Email</th>
            <th scope="col">Block</th>
           
          
          </tr>
        </thead>
        <tbody>
          <h1><listuser[i].name></h1>
          <%listuser.forEach(function(listuser){%>
          <tr>
           
            <td><%= listuser.name%></td>
            <td><%= listuser.email%></td>
            
            
              <% if(listuser.isBlocked==false){%>
              <td><a href="/admin/block/<%=listuser._id%>" class="btn btn-danger" >Block</a></td>
             <% }else{ %>
                <td><a href="/admin/unblock/<%=listuser._id%>"  class="btn btn-danger" >UnBlock</a></td>
            
              <%} %>
           
            
          </tr>
          <%})%>
        </tbody>
        
      </table>
    
  </div>



      
  <%-include('../include/adminresfooter.ejs')-%>